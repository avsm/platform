(library
 (name        which_program)
 (public_name dune.which-program)
 (synopsis    "[Internal] Standard library of Dune")
 (wrapped     false)
 (no_keep_locs)
 (modules_without_implementation which_program))

; To avoid issues on Windows and OSX
(rule (with-stdout-to which_program_dummy.ml (echo "")))
