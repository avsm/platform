(alias
 (name runtest)
 (deps (:t issue798.t) issue798.ml)
 (action
   (progn
     (setenv MERLIN %{exe:../../merlin-wrapper}
       (run %{bin:craml} %{t}))
     (diff? %{t} %{t}.corrected))))

